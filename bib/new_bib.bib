@article{Heemels:October2000:0021-9991:48,
  author = {{Heemels}, M.W. and {Hagen}, M.H.J. and {Lowe}, C.P.},
  title = {Simulating Solid Colloidal Particles Using the Lattice-{B}oltzmann Method},
  journal = {Journal of Computational Physics},
  volume = {164},
  year = {2000},
  month = {October},
  abstract = {The lattice-{B}oltzmann method is a technique for simulating the time-dependent motions of a simple fluid. Introducing rigid particles and imposing the correct boundary conditions at the solid/fluid interface allows the many-body, time-dependent hydrodynamic interactions between particles to be computed. Rather than simulating truly solid particles, a computationally convenient method for doing this uses hollow objects filled with the model fluid. We propose a simple modification of this &#147;internal fluid&#148; method. For computational convenience our method keeps the fluid inside the object. Its behaviour is modified, however, in such a way that it does not perturb the dynamics of the particle. The equations of motion for the solid particles are then modified in such a way that the microscopic conservation laws for mass and momentum are satisfied. Comparing both the time-dependent (rotational and translational) motion of an isolated spherical particle and the viscosity of a concentrated suspension of hard spheres against known results for solid particles, we examine artifacts attributable to the &#147;internal&#148; fluid. Using our modified approach, we show that these artifacts are no longer present and the behaviour of truly solid particles is recovered.},
  pages = {48--61},
  url = {http://www.ingentaconnect.com/content/ap/cp/2000/00000164/00000001/art06564}
}

